{
  "name": "my-inventory-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm-run-all kill-port --parallel dev:backend dev:frontend",
    "kill-port": "kill-port --port 3000 --wait",
    "dev:frontend": "npm --prefix frontend run dev",
    "dev:backend": "npm --prefix backend run dev"
  },
  "devDependencies": {
    "kill-port": "^2.0.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "connect-timeout": "^1.9.0"
  }
}
